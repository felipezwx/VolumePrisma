programa {
  funcao inicio() {
    
   real c, a, l, v

    escreva("Insira o comprimento do prisma : ")
    leia(c)
    limpa()

    escreva("Insira a largura do prisma : ")
    leia(l)
    limpa()
  

    escreva("Insira a altura do prisma : ")
    leia(a)
    limpa()
   
    v = ( c * a * l )

    escreva("O volume do prisma e de : ", v,"m³")


  }
}
